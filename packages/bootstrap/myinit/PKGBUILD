pkgname=myinit
pkgver=1.1.1
pkgrel=1
pkgdesc='minimal init system'
arch=('any')
license=('MIT')
makedepends=('git')
source=("git+https://github.com/TAAPArthur/myinit.git")
sha256sums=('SKIP')
build() {
	cd myinit
    make
}
package() {
	cd myinit
	make DESTDIR=${pkgdir} install
	install -D -m0644 LICENSE ${pkgdir}/usr/share/licenses/${pkgname}/LICENSE
}
